			<script>
				document.getElementById('searchBtn').addEventListener('click', function () {
var keyword = document.getElementById('searchInput').value.toLowerCase();
var formationItems = document.querySelectorAll('.formation-item');

formationItems.forEach(function (item) {
var title = item.querySelector('.card-title').textContent.toLowerCase();
if (title.includes(keyword)) {
item.style.display = 'block';
} else {
item.style.display = 'none';
}
});
});

function filterCourFormation(selectedCourFormation) {
var formationItems = document.querySelectorAll('.formation-item');

formationItems.forEach(function (item) {
var title = item.querySelector('.card-title').textContent.toLowerCase();
if (title === selectedCourFormation.toLowerCase()) {
item.style.display = 'block';
} else {
item.style.display = 'none';
}
});

// Automatically select the matched item in the dropdown
var dropdownItems = document.querySelectorAll('.dropdown-item');
dropdownItems.forEach(function (dropdownItem) {
if (dropdownItem.textContent.toLowerCase() === selectedCourFormation.toLowerCase()) {
dropdownItem.classList.add('active');
} else {
dropdownItem.classList.remove('active');
}
});
}
